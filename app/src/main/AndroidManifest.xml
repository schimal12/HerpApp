<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sebastianchimal.herpapp">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Animales" />
        <activity android:name=".EnciclopediaAnimales" />
        <activity android:name=".EnciclopediaInformacionAnimales" />
        <activity android:name=".Comidas" />
        <activity android:name=".ComidasEnciclopedia" />
        <activity android:name=".AnimalesRegistroConsulta" />
        <activity android:name=".ComidasRegistroConsulta"></activity>
    </application>

</manifest>
